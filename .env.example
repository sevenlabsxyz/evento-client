# Options: 'development', 'production', 'test' - Default: 'development'
NODE_ENV=

# Public API URL - Used by the frontend to make API requests
NEXT_PUBLIC_API_URL=

# API Proxy Target - Used by the server-side API proxy
# Note: This should point to your backend API endpoint
API_PROXY_TARGET=

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Ghost CMS
NEXT_PUBLIC_GHOST_URL=
NEXT_PUBLIC_GHOST_CONTENT_API_KEY=

# Google Maps API Key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=

# Giphy API Key
NEXT_PUBLIC_GIPHY_API_KEY=

# OpenWeatherMap API Key
NEXT_PUBLIC_OPENWEATHERMAP_API_KEY=

# Stream Chat API Key
NEXT_PUBLIC_STREAM_CHAT_API_KEY=

# Breez SDK API Key - Request from https://breez.technology/request-api-key
NEXT_PUBLIC_BREEZ_API_KEY=
